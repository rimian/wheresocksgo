{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "title": "FIXME - Sock",
  "description": "Where socks go",
  "stability": "prototype",
  "strictProperties": true,
  "type": [
    "object"
  ],
  "definitions": {
    "id": {
      "description": "unique identifier of sock",
      "readOnly": true,
      "format": "uuid",
      "type": [
        "string"
      ]
    },
    "name": {
      "description": "unique name of sock",
      "readOnly": true,
      "type": [
        "string"
      ]
    },
    "identity": {
      "anyOf": [
        {
          "$ref": "/schemata/sock#/definitions/id"
        },
        {
          "$ref": "/schemata/sock#/definitions/name"
        }
      ]
    },
    "created_at": {
      "description": "when sock was created",
      "format": "date-time",
      "type": [
        "string"
      ]
    },
    "updated_at": {
      "description": "when sock was updated",
      "format": "date-time",
      "type": [
        "string"
      ]
    }
  },
  "links": [
    {
      "description": "List existing socks.",
      "href": "/socks",
      "method": "GET",
      "rel": "instances",
      "title": "List"
    }
  ],
  "properties": {
    "created_at": {
      "$ref": "/schemata/sock#/definitions/created_at"
    },
    "id": {
      "$ref": "/schemata/sock#/definitions/id"
    },
    "name": {
      "$ref": "/schemata/sock#/definitions/name"
    },
    "updated_at": {
      "$ref": "/schemata/sock#/definitions/updated_at"
    }
  },
  "id": "schemata/sock"
}
